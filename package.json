{
  "name": "evolutics-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build",
    "build-auth-dev": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration=auth-development",
    "build-auth-prod": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration=auth-production",
    "build-trans-prod": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration=translation-production",
    "build-sandbox-prod": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration=sandbox-prod",
    "build-general": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration=general",
    "serve": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng serve",
    "serve-trans": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng serve --configuration=translation-development",
    "serve-auth": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng serve --configuration=auth-development",
    "serve-prod": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng serve --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "local": "ng serve",
    "start-host": "ng serve --host 0.0.0.0"
  },
  "private": true,
  "dependencies": {
